platform :ios, '7.0'
inhibit_all_warnings!

target 'WilliamProject' do

#辅助工具 ###########################################################################################

pod 'AFNetworking',       '>= 2.6.0'      #网络请求库    https://github.com/Haneke/Haneke-AFNetworkinge
pod 'SVPullToRefresh',    '~> 0.4.1'      #下拉刷新      https://github.com/samvermette/SVPullToRefresh
pod 'YYKit',              '>= 0.9.7'      #YYKit        https://github.com/ibireme/YYKit
pod 'YTKNetwork',         '>= 1.1.0'      #猿题库网络封装 https://github.com/yuantiku/YTKNetwor
pod 'PSTAlertController', '>= 1.2.0'      #iOS7可以使用的AlertController
pod 'MMPopupView',        '>= 1.6'        #可自定义UI的系统效果Alert、ActionSheet https://github.com/adad184/MMPopupView
pod 'MJRefresh',          '~> 3.1.12'     #MJ刷新控件    https://github.com/CoderMJLee/MJRefresh
#界面 ###############################################################################################

pod 'Masonry',            '>= 1.0.1'      #自动布局   https://github.com/cloudkite/Masonry
pod 'DZNEmptyDataSet',    '>= 1.7.3'      #Table无数据页面
pod 'PDTSimpleCalendar',  '~> 0.9.1'      #日历       https://github.com/jivesoftware/PDTSimpleCalendar
pod 'MWPhotoBrowser', 	  '~> 2.1.2'      #图片浏览
pod 'QBImagePickerController', '~> 2.6.0' #图片选择    https://github.com/questbeat/QBImagePicker

end
