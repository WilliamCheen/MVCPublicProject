pod 'YYKit',              '~> 0.9.7'      #YYKit       https://github.com/ibireme/YYKit
pod 'YTKNetwork',         '~> 0.5.0'      #猿题库网络封装 https://github.com/yuantiku/YTKNetwor
pod 'PSTAlertController', '~> 1.2.0'      #iOS7可以使用的AlertController
pod 'MMPopupView',        '~> 1.6'        #可自定义UI的系统效果Alert、ActionSheet https://github.com/adad184/MMPopupView

#界面 ###############################################################################################

pod 'Masonry',            '~> 0.6.3'      #自动布局   https://github.com/cloudkite/Masonry
